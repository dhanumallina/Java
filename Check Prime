                           /* Given an integer n check if n is prime or not. */
class Solution {
    public static boolean prime(int n) 
    {
        // Write your code here
        if (n <= 1) return false;
        if (n == 2) return true;  // 2 is prime
        if (n % 2 == 0) return false;
        for (int i = 3; i * i <= n; i += 2) 
        {
            if (n % i == 0) return false;
        }
        return true;
        
    }
}
